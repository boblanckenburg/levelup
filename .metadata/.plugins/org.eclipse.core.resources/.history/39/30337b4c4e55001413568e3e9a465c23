
function changeProgress()
{
	var percentage = Math.random() * 100;
	
	var progress_fill = document.getElementById("levelcounter_progressbar");
	var percentage = document.getElementById("levelcounter_percentage");
	
	percentage.innerHTML = percentage + "%";
	progress_fill.style.width = percentage + "%";
	
	alert( "percentage " + percentage );
}

changeProgress();

